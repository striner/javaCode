import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Scanner input = new Scanner(System.in);
		int n = input.nextInt();
		int[] m = new int[n];
		for (int i = 0; i < n; i++) {
			m[i] = input.nextInt();
		}
		int a = input.nextInt();
		input.close();
		
		for (int i = 0; i < n; i++) {
			if (a == m[i]) {
				System.out.println(i+1);
				return;
			} 
		}
		System.out.println("-1");
	}

}
